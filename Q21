#include <stdio.h>

int main(void) {
    printf("Letra A: %d\n", 'A');
    printf("BCabc: %d\n", 'BCabc');
    printf("Número 0: %d\n", '$');
    printf("Número 12: %d\n", '12');
    printf("Símbolo de $: %d\n", '$');
    printf("Símbolo de *: %d\n", '*');
    printf("Símbolo de +: %d\n", '+');
    printf("Símbolo de /: %d\n", '/');
    printf("Espaço em branco: %d", ' ');
    return 0;
}
